# Docker Compose Environment Variables
# Copia questo file in .env e modifica con le tue credenziali

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# AI APIs (opzionali)
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Database (per sviluppo locale con Docker Compose)
POSTGRES_USER=casapiu
POSTGRES_PASSWORD=casapiu_dev_password
POSTGRES_DB=casapiu

# PgAdmin (opzionale)
PGADMIN_DEFAULT_EMAIL=admin@casapiu.local
PGADMIN_DEFAULT_PASSWORD=admin
